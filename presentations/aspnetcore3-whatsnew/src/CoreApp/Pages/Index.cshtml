@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div>
    <h1>Name: @Model.Endpoint.DisplayName</h1>
    <table class="table">
        @foreach(var data in Model.Endpoint.Metadata)
        {
        <tr>
            <td>@data.GetType().Name</td>
            <td>
                <partial name="@data.GetType().Name" optional="true" model="@data" />
            </td>
        </tr>
        }
    </table>
</div>
